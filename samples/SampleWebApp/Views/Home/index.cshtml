<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Key</th>
            <th scope="col">Value</th>
        </tr>
    </thead>
    <tbody>
        @{
            var config = ViewData["config"] as Dictionary<string, string>;
            foreach (var (key, value) in config)
            {
                <tr>
                    <td>@key</td>
                    <td>@value</td>
                </tr>
            }
        }
    </tbody>
</table>
